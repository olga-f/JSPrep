{
  "name": "jsprep-front-end",
  "version": "1.0.0",
  "description": "JS Prep front-end",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "jest": "jest --selectProjects client",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --selectProjects client --watch",
    "type-check": "tsc",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|jsx|md|ts|tsx)\"",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "validate": "npm-run-all --parallel type-check test:coverage",
    "codegen:schema": "npx apollo client:download-schema --endpoint=http://127.0.0.1:8000/graphql/",
    "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=lib/**/*.ts --globalTypesFile=./lib/graphql/globalTypes.ts --target=typescript",
    "setup": "npm ci && npm run validate"
  },
  "dependencies": {
    "@apollo/client": "latest",
    "@codemirror/basic-setup": "latest",
    "@codemirror/lang-javascript": "latest",
    "@codemirror/theme-one-dark": "latest",
    "@codemirror/view": "latest",
    "apollo-boost": "latest",
    "baseui": "latest",
    "graphql": "latest",
    "js-beautify": "latest",
    "lodash": "latest",
    "next": "latest",
    "next-seo": "latest",
    "react": "latest",
    "react-dom": "latest",
    "styletron-engine-atomic": "latest",
    "styletron-react": "latest"
  },
  "devDependencies": {
    "@testing-library/cypress": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/jest": "latest",
    "@types/js-beautify": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/styletron-engine-atomic": "latest",
    "@types/styletron-react": "latest",
    "@types/styletron-standard": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-jest": "latest",
    "cypress": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-cypress": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jest-dom": "latest",
    "eslint-plugin-react": "latest",
    "is-ci-cli": "latest",
    "jest": "latest",
    "jest-runner-eslint": "latest",
    "jest-watch-select-projects": "latest",
    "jest-watch-typeahead": "latest",
    "npm-run-all": "latest",
    "prettier": "latest",
    "start-server-and-test": "latest",
    "typescript": "latest"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": ".gitignore"
    }
  },
  "keywords": [
    "react",
    "react.js",
    "next.js",
    "graphql",
    "apollo"
  ],
  "author": "Olga F. Andreiko",
  "license": "MIT"
}
