{
  "name": "jsprep-front-end",
  "version": "1.0.0",
  "description": "JS Prep front-end",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "jest": "jest --selectProjects client",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --selectProjects client --watch",
    "type-check": "tsc",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|jsx|md|ts|tsx)\"",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "validate": "npm-run-all --parallel type-check test:coverage",
    "codegen:schema": "npx apollo client:download-schema --endpoint=http://127.0.0.1:8000/graphql/",
    "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=lib/**/*.ts --globalTypesFile=./lib/graphql/globalTypes.ts --target=typescript",
    "setup": "npm ci && npm run validate"
  },
  "dependencies": {
    "@apollo/client": "^3.3.18",
    "@codemirror/basic-setup": "^0.18.1",
    "@codemirror/lang-javascript": "^0.18.0",
    "@codemirror/view": "^0.18.12",
    "apollo-boost": "^0.4.9",
    "baseui": "^9.115.1",
    "graphql": "^15.5.0",
    "javascript-code-runner": "^1.0.5",
    "js-beautify": "^1.13.13",
    "lodash": "^4.17.21",
    "next": "^10.2.3",
    "next-seo": "^4.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-react": "^6.0.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/js-beautify": "^1.13.1",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^26.6.3",
    "cypress": "^7.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^26.6.3",
    "jest-runner-eslint": "^0.10.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "start-server-and-test": "^1.12.2",
    "typescript": "^4.2.4"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": ".gitignore"
    }
  },
  "keywords": [
    "react",
    "react.js",
    "next.js",
    "graphql",
    "apollo"
  ],
  "author": "Olga F. Andreiko",
  "license": "MIT"
}
